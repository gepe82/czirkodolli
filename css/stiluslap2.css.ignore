.temadoboz{    
    float: left;
    width: 141px;
    padding-top: 20px;
    padding-left: 30px;
    height: 132px;    
    border-left: 1px solid #fff;    
    border-radius: 10px;    
    line-height: 200%;
    font-size: 0.9em;
    color: rgba(0, 0, 0, 0.7);
    
}

#sor1 .temadoboz{
    border-top: 1px solid #fff;
    box-shadow: -4px -5px 8px 1px rgba(0, 0, 0, 0.3);
    -moz-transition-property: border-top-left-radius, filter;
    -moz-transition-duration: 0.5s;
    -webkit-transition-property: border-top-left-radius, filter;
    -webkit-transition-duration: 0.5s;
}

#sor1 .temadoboz:hover{    
    border-top-left-radius: 50px;
    /*-webkit-filter: opacity(0.95);*/
}


#sor2 .temadoboz{
  border-bottom: 1px solid #fff;
  box-shadow: -4px 5px 8px 1px rgba(0, 0, 0, 0.3);
  -moz-transition-property: border-bottom-left-radius;
  -moz-transition-duration: 0.5s;
  -webkit-transition-property: border-bottom-left-radius, filter;
  -webkit-transition-duration: 0.5s;
}

#sor2 .temadoboz:hover{    
    border-bottom-left-radius: 50px;
    /*-webkit-filter: opacity(0.95);*/
}

