html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video, main {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
}

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, main {
  display: block;
}

#befoglalo {
  width: 100%;
  margin-top: 0;
  box-sizing: border-box;
}

h1 {
  font-size: 1.5em;
  padding: 25px 0 15px 100px;
  text-align: left;
  font-weight: bold;
  white-space: nowrap;

  color: rgba(0, 0, 0, 0.5);
  margin: 0 auto;
}

a, a:link, a:hover, a:visited, a:active {
  text-decoration: none;
  color: rgba(0, 0, 0, 0.5);
}

section {
  width: 695px;
  clear: both;
  margin: 0 auto;

}

section > div > ul {
  list-style-image: url('img/list-l.jpg');
  -moz-transition-property: list-stlye-image;
  -webkit-transition-property: list-style-image;
}

section > div > ul li:hover{
  list-style-image: url('img/list-r.jpg');
  -webkit-transition-duration: 0.75s;
  -moz-transition-duration: 0.75s;
}

nav {
  overflow: hidden;
  width: 100%;
  clear: both;
  background-color: hsla(48, 100%, 51%, 0.8);
  height: 45px;
  border-top: 1px solid #fff;
  margin: 0 auto;
  box-shadow: 0 0px 2px 3px rgba(0, 0, 0, 0.15);
  max-width: none;
  min-height: 0;

  z-index: -1;
  position: absolute;
  margin-top: 153px;
}

nav > ul {
  margin: 0 auto;
  list-style: none;
  width: 690px;
  height: 45px;
  border-right: 5px solid hsla(48, 100%, 51%, 0.8);
}

nav > ul li {
  float: left;
  width: 171px;
  height: 45px;
  clear: none;

  border-left: 1px solid #fff;
  font-size: 1.125em;
  font-weight: bold;
  border-radius: 5px;
  color: #fff;

  -webkit-transition-property: -webkit-transform;
  -webkit-transform-style: preserve-3d;
  -moz-transition-property: -moz-transform;
  -moz-transform-style: preserve-3d;
}

nav li a {
  display: block;
  padding: 0.7em 0 0 50px;
}

.temadoboz{
  border-left: 1px solid #fff;
  border-radius: 10px;
  color: rgba(0, 0, 0, 0.7);
  float: left;
  width: 171px;
  height: 152px;
}

.temadoboz ul {
  line-height: 120%;
  font-size: 1.1em;
  padding: 1em 0.5em 1em 1.5em;
}

.temadoboz li {
  margin-bottom: 0.3em;
}

.temadoboz li a:hover,
  .temadoboz li a:active,
  .temadoboz li a:focus{
  text-decoration: underline;
  }

.temadoboz img{
  width: 100%;
  height: auto;
  max-width: 100%;
  border-radius: 10px;
  z-index: -1;
  display: block;
  max-width: 171px;
  max-height: 152px;
}

#d1 {
    border-top: 1px solid #fff;
    box-shadow: -4px -5px 8px 1px rgba(0, 0, 0, 0.3);
}

#d2 {
    margin-top: 47px;
    clear: left;
    box-shadow: -4px 5px 8px 1px rgba(0, 0, 0, 0.3);
}

#sor1 .temadoboz,
  #sor1 .temadoboz img {
  -moz-transition-property: border-top-left-radius; /*, filter*/
  -moz-transition-duration: 0.5s;
  -webkit-transition-property: border-top-left-radius; /*, filter*/
  -webkit-transition-duration: 0.5s;
}

#sor1 .temadoboz {

}

#sor1 .temadoboz:hover,
  #sor1 .temadoboz:focus,
  #sor1 .temadoboz img:hover {
  border-top-left-radius: 50px;
  /*-webkit-filter: opacity(0.95);*/
}

#sor2 .temadoboz,
  #sor2 .temadoboz img {
  -moz-transition-property: border-bottom-left-radius;
  -moz-transition-duration: 0.5s;
  -webkit-transition-property: border-bottom-left-radius;
  -webkit-transition-duration: 0.5s;
}

#sor2 .temadoboz{
  border-bottom: 1px solid #fff;
  box-shadow: -4px 5px 8px 1px rgba(0, 0, 0, 0.3);
}

#sor2 .temadoboz:hover,
  #sor2 .temadoboz:focus,
  #sor2 .temadoboz img:hover {
  border-bottom-left-radius: 50px;
  /*-webkit-filter: opacity(0.95);*/
}


#fejlec{
  background: #fff url('img/logo.jpg') no-repeat 30px center;
  width: 690px;
  margin: 50px auto 25px;
  height: 50px;

  -webkit-animation-name: beuszas;
  -webkit-animation-duration: 2s;
  -webkit-animation-timing-function: ease-in-out;
  animation-name: beuszas;
  animation-duration: 2s;
  animation-timing-function: ease-in-out;
}

#lablec{
  width: 690px;
  clear: both;
  text-align: right;
  font-size: 0.75em;
  color: rgba(0, 0, 0, 0.5);
  margin: 0 auto;
  padding: 1.5em 3em 0 0;
}

#magamrol {
  background: hsl(199, 38%, 57%) url('img/ab2.jpg') no-repeat 10px center;
}

main {
  float: right;
  background-color: hsl(199, 38%, 57%);
  border: 1px solid red;
  width: 521px;
  min-height: 353px;
  border-radius: 10px;
  border: 1px solid #fff;
  box-shadow: -2px -2px 10px 4px rgba(0, 0, 0, 0.3);
}

main p {
  background-color: #fff;
  margin: 10px 10px 10px 15px;
  padding: 10px;
  text-indent: 1.5em;
  border-radius: 5px;
  box-shadow: -1px -1px 5px 2px rgba(0, 0, 0, 0.3);
}